{
    "name": "aanshi/laravel-js-localization",
    "description": "Laravel Localization in JavaScript",
    "type": "library",
    "keywords": [
        "laravel",
        "laravel 5",
        "localization",
        "i18n",
        "javascript",
        "js",
        "lang"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aanshi Evangeline",
            "email": "aanshievangeline@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.4 || ^7.0 || ^8.0",
        "illuminate/config": "^4.2 || ^5.0 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/console": "^4.2 || ^5.0 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/filesystem": "^4.2 || ^5.0 || ^6.0 || ^7.0 || ^8.0",
        "tedivm/jshrink": "~1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^2.2 || ^3.0 || ^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Aanshi\\LaravelJsLocalization\\": "src/Aanshi/LaravelJsLocalization/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aanshi\\LaravelJsLocalization\\": "tests/specs"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Aanshi\\LaravelJsLocalization\\LaravelJsLocalizationServiceProvider"
            ]
        }
    }
}
